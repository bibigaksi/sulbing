	/*layout*/


	
	body{position:relative}





	/*home*/


	#home {height:960px; width:100%; background:url('../images/headerBackground.png') no-repeat; background-size:100% 100%; position:relative}

	#home h1 {font-family: 'Gloria Hallelujah', cursive; position:absolute; left:62%; top:27%;letter-spacing:1px; color:#56462a; font-size:40px}
	#home .writing {font-family: 'Gloria Hallelujah', cursive; position:absolute; left:32%; top:17%; font-size:60px; font-weight:bold; letter-spacing:5px; color:#8b7054}


	 .Logo  {text-align:center; width:160px; background:rgba(255,215,157,0.3); border-radius:7px; position:fixed; top:35px; left:40px; transition:0.3s; padding:10px}
	 .Logo:hover  {background:rgba(254,210,152,1); }
	 
	 .Logo .logo { width:130px; height:70px;}
	 .gnb {position:fixed; top:400px; left:0; color:#5b3400; }
	 .gnb li { border-bottom:2px solid #733900; padding:30px 20px 0 100px; font:20px 'Gloria Hallelujah', cursive; line-height:8px }
	 .gnb li a {display:block; width:100px; height:15px; text-align:right; }
	 .gnb li a:hover {color:#ff3e3e; font-size:25px; transform:translate(10px, -2px); transition:0.5s}
	 .gnb li:nth-child(4) a:hover {transform:translate(-15px, -2px); }
	 .gnb li:nth-child(5) a:hover {transform:translate(-7px, -2px); }
	 .gnb_T {display:none}
	 .gnb_P {display:none}

	
	#home .Ground {position:absolute; bottom:0; left:0; width:100%; height:250px; } 
	#home .ground { width:100%; height:250px; }

	#home .snowG1 {position:absolute; bottom:-110px; left:-90px;  width:600px; height:230px;}
	#home .Bingsu1 {position:absolute; top:50%; left:50%; margin-top:-30px; margin-left:-230px ; } 
	#home .bingsu1 { width:460px; height:350px; }
	#home .cloud1 {position:absolute; bottom: 300px; right:-50px; 
	animation:cloud 0.5s infinite alternate both;
	-webkit-animation:cloud 0.5s infinite alternate both;
	-ms-animation:cloud 0.5s infinite alternate both;
	-moz-animation:cloud 0.5s infinite alternate both;
	-o-animation:cloud 0.5s infinite alternate both;
	opacity:0	}


	#home .Bingsu1:hover .cloud1{opacity:1}


	@keyframes cloud{
	0% {transform:rotate(-7deg)}
	100% {transform:rotate(7deg)}
	}

	#home .snow {width:100%; height:960px; position:static; margin-left:0; margin-top:0; opacity:1}

